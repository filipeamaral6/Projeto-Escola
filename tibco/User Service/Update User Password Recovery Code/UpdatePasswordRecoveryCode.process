        <pd:x>434</pd:x>
        <pd:y>113</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>UPDATE USERS SET PASSWORD_RECOVERY_CODE=(?) WHERE USERNAME=(?)</statement>
            <jdbcSharedConfig>/Database/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>PASSWORD_RECOVERY_CODE</parameterName>
                    <dataType>VARCHAR</dataType>
