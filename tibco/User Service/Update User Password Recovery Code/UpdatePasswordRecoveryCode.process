        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <PASSWORD_RECOVERY_CODE>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:UpdatePasswordRecoveryCodeInput/input/ns:InputData/@code)">
